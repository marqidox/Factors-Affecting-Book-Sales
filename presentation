    "import matplotlib.pyplot as plt \n",
    "from pylab import array, plot\n",
    "\n",
    "# organizing the data to be plotted\n",
    "prices=top50Books.Price.values\n",
    "user_ratings=top50Books['User Rating'].values\n",
    "pairs=list(zip(prices,user_ratings))\n",
    "\n",
    "# getting the bins\n",
    "maxiumum_rating=round(user_ratings.max())\n",
    "bins=list(range(maxiumum_rating))\n",
    "\n",
    "#plotting it\n",
    "#plt.plot(*zip(*pairs))\n",
    "plt.hist(prices,bins)\n",
    "plt.xlabel('User Rating')\n",
